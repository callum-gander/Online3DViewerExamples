{
	"name": "online-3d-viewer-esm-example",
	"description": "Online 3D Viewer",
	"version": "0.0.1",
	"repository": "github:kovacsv/Online3DViewer",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"start": "npm run build && http-server",
		"copylibs": "copyfiles --up 3 node_modules/online-3d-viewer/libs/loaders/*.* build/libs",
		"build": "esbuild source/main.js --bundle --minify --sourcemap --outfile=build/bundle.js && npm run copylibs"
	},
	"devDependencies": {
		"copyfiles": "^2.4.1",
		"esbuild": "^0.14.23",
		"http-server": "^14.0.0"
	},
	"dependencies": {
		"online-3d-viewer": "*"
	}
}
